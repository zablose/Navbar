{
    "name": "zablose/navbar",
    "description": "Navbar - Build an HTML from the navigation entities, stored in a database.",
    "keywords": [
        "navbar",
        "navigation",
        "html",
        "builder"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergejs Zablockis",
            "email": "zablose@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "illuminate/database": "^8.12.3",
        "phpunit/phpunit": "^9.4.2",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "Zablose\\Navbar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zablose\\Navbar\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
