{
    "name": "zablose/navbar",
    "description": "Navbar - Build an HTML from the navigation entities, stored in a database.",
    "keywords": [
        "navbar",
        "navigation",
        "html",
        "builder"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergejs Zablockis",
            "email": "zablose@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "facade/ignition": "^2.5.0",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.3",
        "fzaninotto/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.2.0",
        "laravel/framework": "^8.13.0",
        "laravel/tinker": "^2.5.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.1.0",
        "phpunit/phpunit": "^9.4.2",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "Zablose\\Navbar\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "laravel/app/",
            "Zablose\\Navbar\\Tests\\": "tests/"
        },
        "classmap": [
            "laravel/database/seeders"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
